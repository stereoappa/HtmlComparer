<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="pages" type="HtmlComparer.Sections.PagesSection, HtmlComparer" />
		<section name="sources" type="HtmlComparer.Sections.SourcesSection, HtmlComparer" />
		<section name="compareFields" type="HtmlComparer.Sections.CompareFieldsSection, HtmlComparer" />
	</configSections>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>

	<sources>
		<source baseUrl="https://realrussia.co.uk" compare-role="Origin" />
		<source baseUrl="https://new.realrussia.co.uk" return-url-lower-case="true" compare-role="Target" />
	</sources>

	<pages>
		<page path="/Info/Faq" />
		<page path="/Info/TravelSim" />
		<page path="/Info/Testimonials" />
		<page path="/Info/HowToPay" />
		<page path="/Info/Languages" />
		<page path="/Info/Links" />
		<page path="/Info/Money" />
		<page path="/Info/Money/Budget-Russia" />
		<page path="/Info/Money-Back-Guarantee" />
		<page path="/Info/Real-Russia-Story" />
		<page path="/Info/Testimonials/Video" />
		<page path="/Info/Transfers" />
		<page path="/Info/TravelInsurance" />
		<page path="/Info/Why-Real-Russia" />
		<page path="/Info/WorldTravelAwards" />
		<page path="/Info/About-Us" />
		<page path="/Info/Contact-Us" />
		<page path="/Info/Crimea-Update" />
	</pages>

	<compareFields>
		<field tag="meta" name="keywords" attr="content"/>
		<field tag="meta" name="description" attr="content"/>
		<field tag="title" />
	</compareFields>
</configuration>